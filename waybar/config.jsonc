{
  "layer": "top",
  "position": "top",
  "height": 26,
  "spacing": 0,
  "exclusive": true,
  "gtk-layer-shell": true,
  "passthrough": false,
  "fixed-center": true,
  "modules-left": ["custom/logo", "hyprland/workspaces", "hyprland/window"],
  "modules-right": ["pulseaudio", "clock", "tray"],

  "hyprland/window": {
